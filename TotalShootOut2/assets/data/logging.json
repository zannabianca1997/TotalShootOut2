{
  "version": 1,
  "formatters": {
    "detailed": {
      "format": "%(asctime)s:%(name)s:%(levelname)s:%(message)s"
    },
    "simple": {
      "format": "%(name)s:%(levelname)s:%(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "WARNING",
      "formatter":"simple",
      "stream": "ext://sys.stderr"
    },
    "file": {
      "class": "logging.FileHandler",
      "level": "DEBUG",
      "formatter":"detailed",
      "filename": "out.log",
      "mode": "w"
    }
  },
  "loggers": {
  },
  "root": {
    "handlers": [
      "console",
      "file"
    ],
    "level":"DEBUG"
  }
}